{
  "name": "nodejs-angular-2",
  "description": "NodeJS/Angular-2 Boilerplate",
  "version": "0.0.1",
  "scripts": {
    "app-requirements": "npm run global && npm run required",
    "global": "npm install nodemon -g && npm install bower -g && npm install gulp -g && npm install typings -g && npm install typescript -g && npm install pm2 -g",
    "required": "typings install && npm install && bower install",
    "dev-compile": "tsc 2>&1 && gulp settings && tsc && gulp dev-compile && tsc",
    "prod-compile": "tsc 2>&1 && gulp settings && tsc && gulp prod-compile",
    "gitadd": "rm -rf gulpfile.js gulp.config.js && git add .",
    "app": "pm2 start back/app.server.js -o ~/LOGS/.appoutput -e ~/LOGS/.apperror --name APP -i",
    "useforwindowcompilation": "",
    "windows-compile": "gulp less-compile-and-minify && gulp assetsJs && gulp appJs && gulp html-min && tsc"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/router": "3.0.2",
    "body-parser": "1.15.2",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "core-js": "2.4.1",
    "ejs": "2.5.2",
    "express": "4.14.0",
    "jwt-simple": "0.5.0",
    "method-override": "2.3.6",
    "moment": "^2.15.1",
    "morgan": "1.7.0",
    "ng2-translate": "3.1.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "^2.3.0",
    "systemjs": "0.19.39",
    "typescript": "2.0.3",
    "winston": "2.2.0",
    "yargs": "6.0.0",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "del": "2.2.2",
    "glob": "7.1.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-clean-css": "2.0.13",
    "gulp-csslint": "1.0.0",
    "gulp-htmlmin": "3.0.0",
    "gulp-if": "2.0.1",
    "gulp-inject": "4.1.0",
    "gulp-less": "3.1.0",
    "gulp-load-plugins": "1.3.0",
    "gulp-notify": "2.2.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "1.7.3",
    "gulp-uglify": "2.0.0",
    "gulp-useref": "3.1.2",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.10",
    "lazypipe": "1.0.1",
    "less-plugin-autoprefix": "1.5.1",
    "minimatch": "3.0.3",
    "run-sequence": "1.2.2",
    "wiredep": "4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "yourhttpurl"
  }
}
