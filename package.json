{
  "name": "nodejs-angular-2",
  "description": "NodeJS/Angular-2 Boilerplate",
  "version": "0.0.1",
  "scripts": {
    "app-requirements": "npm run global && npm run required",
    "global": "npm install nodemon -g && npm install bower -g && npm install gulp -g && npm install typings -g && npm install typescript -g && npm install pm2 -g",
    "required": "typings install && npm install && bower install",
    "dev-compile": "tsc 2>null && gulp settings 2>null && tsc && gulp dev-compile && tsc",
    "prod-compile": "tsc 2>null && gulp settings 2>null && tsc && gulp less-compile-and-minify && gulp prod-compile && tsc && gulp uglify",
    "gitadd": "rm -rf gulpfile.js gulp.config.js && git add .",
    "app": "pm2 start back/app.server.js -o ~/LOGS/.appoutput -e ~/LOGS/.apperror --name APP -i",
    "useforwindowcompilation": "",
    "windows-compile": "gulp less-compile-and-minify && gulp assetsJs && gulp appJs && gulp html-min && tsc"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/forms": "2.4.6",
    "@angular/http": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/upgrade": "2.4.6",
    "@angular/router": "3.4.6",
    "body-parser": "1.16.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "core-js": "2.4.1",
    "ejs": "2.5.5",
    "express": "4.14.1",
    "jwt-simple": "0.5.1",
    "method-override": "2.3.7",
    "moment": "2.17.1",
    "morgan": "1.8.0",
    "ng2-translate": "5.0.0",
    "reflect-metadata": "0.1.9",
    "rxjs": "5.0.1",
    "serve-favicon": "2.3.2",
    "systemjs": "0.20.5",
    "typescript": "2.0.9",
    "winston": "2.3.1",
    "yargs": "6.6.0",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "del": "2.2.2",
