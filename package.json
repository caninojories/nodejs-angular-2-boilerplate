    "gulp-useref": "3.1.2",
    "gulp-util": "3.0.8",
    "gulp-watch": "4.3.11",
    "lazypipe": "1.0.1",
    "less-plugin-autoprefix": "1.5.1",
    "minimatch": "3.0.3",
    "run-sequence": "1.2.2",
    "wiredep": "4.0.0"
